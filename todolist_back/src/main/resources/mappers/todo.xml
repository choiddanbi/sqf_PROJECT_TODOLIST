<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.todolist.repository.TodoMapper">

    <insert id="save">
        insert into todo_tb
        values (
            0,
            #{userId},
            #{title},
            #{content},
            #{important},
            #{busy},
            1,
            #{todoDateTime},
            now(),
            now()
        )
    </insert>
</mapper>